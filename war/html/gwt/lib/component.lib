nd(\"<script type='text\/javascript' src='\"+gk.ctx+\"\/html\/gwt\/res\/codemirror\/mode\/gulmixed\/gulmixed.js'\/>\");\n      $(\"head\").append(\"<script type='text\/javascript' src='\"+gk.ctx+\"\/html\/gwt\/res\/codemirror\/mode\/autocomplete\/autoComplete.js'\/>\");\n      \n      if(document.createStyleSheet){\n\tdocument.createStyleSheet(gk.ctx+\"\/html\/gwt\/res\/codemirror\/lib\/codemirror.css\");\n\tdocument.createStyleSheet(gk.ctx+\"\/html\/gwt\/res\/codemirror\/mode\/autocomplete\/autoComplete.css\");\n\tdocument.createStyleSheet(gk.ctx+\"\/html\/gwt\/res\/codemirror\/mode\/xml\/xml.css\");\n\tdocument.createStyleSheet(gk.ctx+\"\/html\/gwt\/res\/codemirror\/theme\/deftheme.css\");\n      }else {\n\t$(\"head\").append($(\"<link rel='stylesheet' href='\"+gk.ctx+\"\/html\/gwt\/res\/codemirror\/lib\/codemirror.css'\/>\"));\n\t$(\"head\").append($(\"<link rel='stylesheet' href='\"+gk.ctx+\"\/html\/gwt\/res\/codemirror\/mode\/autocomplete\/autoComplete.css'\/>\"));\n\t$(\"head\").append($(\"<link rel='stylesheet' href='\"+gk.ctx+\"\/html\/gwt\/res\/codemirror\/mode\/xml\/xml.css'\/>\"));\n\t$(\"head\").append($(\"<link rel='stylesheet' href='\"+gk.ctx+\"\/html\/gwt\/res\/codemirror\/theme\/deftheme.css'\/>\"));\n      }\n    }\n    \n    var TAeditor = CodeMirror.fromTextArea(document.getElementById(\"CMTA-input\"),\n\t\t\t\t\t   {\n\t\t\t\t\t     mode:\"text\/html\",\n\t\t\t\t\t     tabMode :\"indent\",\n\t\t\t\t\t     matchBrackets: true,\n\t\t\t\t\t     lineNumbers: true,\n\t\t\t\t\t     fixedGutter: true,\n\t\t\t\t\t     onCursorActivity :function(TAeditor){TAeditor.onCurActivity()},\n\t\t\t\t\t     onGutterClick: function(TAeditor, line, e){TAeditor.doFoldFunc(line, e)},\n\t\t\t\t\t     onKeyEvent: function(TAeditor, e ) {\n\t\t\t\t\t       if(e.type==\"keydown\"){\n\t\t\t\t\t\t \/\/ Hook into alt+\/  , keyCode ==191 is \"\/\"key\n\t\t\t\t\t\t if (e.keyCode == 191 && e.altKey){\n\t\t\t\t\t\t   e.stop();\/\/prevent explore keyboard shortcut\n\t\t\t\t\t\t   return TAeditor.startComplete();\n\t\t\t\t\t\t } \t \n\t\t\t\t\t\t if (e.keyCode == 40 && e.altKey && e.ctrlKey) return TAeditor.copySelDown(e);\n\t\t\t\t\t\t if (e.keyCode == 38 && e.altKey && e.ctrlKey) return TAeditor.copySelUp(e);\n\t\t\t\t\t\t \/\/Hook into ctrl+D delete a line\n\t\t\t\t\t\t if (e.keyCode == 68 && e.ctrlKey) return TAeditor.deleteLine(e);\n\t\t\t\t\t\t if (e.keyCode == 38 && e.altKey ) return TAeditor.moveUpLine(e);\/\/alt+up\n\t\t\t\t\t\t if (e.keyCode == 40 && e.altKey ) return TAeditor.moveDownLine(e);\/\/alt+down\n\t\t\t\t\t\t if (e.keyCode == 82 && e.shiftKey && e.altKey ) return TAeditor.selReplace(e);\/\/alt+shift+R\n\t\t\t\t\t\t if (e.keyCode == 191 && e.ctrlKey  ) return TAeditor.addComment();\/\/ctrl+\/\n\t\t\t\t\t\t if (e.keyCode == 66 && e.ctrlKey  ) return TAeditor.markBlockSwitch(e);\/\/ctrl+B   start or stop mark block;\n\t\t\t\t\t       }\n\t\t\t\t\t     }\n\t\t\t\t\t   });\n    \n    $(\"#CMTA > div > .CodeMirror-scroll\").css({\"height\":\"100%\",\"width\":\"100%\",\"position\":\"absolute\",\"top\":\"0px\",\"left\":\"0px\",\"background-color\":\"#fff\"});\n    TAeditor.refresh();\n    \n    ]]>\n    \n  <\/js>\n<\/page>","gkDesignWizard_Layout":"<page >\n  \n  <window draggable='true' id='designWizardLayout' heading='版型設計精靈' layout='vrow' width='650' height='600' resizable='false' modal='true'>\n    \n    <form headerVisible='false' style='padding:0px' id='designWizardLayoutForm'>\n      <toolbar id='gkmwDesignWizardLayoutTool'   separator='true' >\n        <field type='btn' label='完成' onClick='js:finishWizardLayout'\/>\n        <field type='btn' label='取消' onClick='js:closeLayoutWizard'\/> \n        <field id ='layoutValue' name='layoutValue' type='txt' value='' label='選擇的面版' borders='true'\/>\n      <\/toolbar>\n      \n      <contentPanel layout='vrow' height='130' scroll='auto' headerVisible='false'>     \n        <contentPanel heading='A' bodyBorder='true' headerVisible='false' >\n          <hp width='100%'  borders='true