\n\t.mouseup(function (e) {\n\t  drawMode = false;\n\t});\n    }\n    \n  <\/js>\n  <js id='view0'>\n    view0();\n  <\/js>\n  \n  <js id='paint'>\n    paint();\n  <\/js>\n<\/page>","子元件擺放":"<panel onDrop='js:put' layout='fit' >\n  <hp>\n    <table width='100%' style='border-width:1px; border-style:dotted;border-color:red;background-color:white;'><tr>\n      <td align='center'> \n        元件擺放處       \n      <\/td>\n      <\/tr>\n    <\/table>\n  <\/hp>\n<\/panel>\n","onlineHelp_cheatsheet":"<page layout='fit'>\n  <contentPanel headerVisible='false'>\n  <hp>\n    <![CDATA[\n    <body class='ohbody' marginwidth=\"5\" marginheight=\"5\" style='margin: 0px;overflow:auto;'>\n    <link rel='stylesheet' type='text\/css' href='.\/html\/gwt\/res\/gk\/onlineHelp\/css\/onlineHelp.css'\/>\n      <div style='padding-left:2px'>\n        <div class=\"col\" style='float:left;width: 136px;overflow:hidden;margin-right: 2px;'>\n          <div style='cursor:pointer;'>\n            <h2 class='ohh2' style='font-size: 15px;font-weight: normal;padding:0;margin:0;list-style:none;font-family:Verdana,新細明體,Arial,sans-serif,\"新細明體\";line-height: 1.2;'><a class='oha' style='color:#bed742;' onclick=\"_gkmh_linkCont('A')\">共通屬性<\/a><\/h2>        \n            <ul class='uls'>\n              <li><a class='oha' onclick=\"_gkmh_linkCont('A_borders')\">borders<\/a><\/li>\n              <li><a class='oha' onclick=\"_gkmh_linkCont('A_enable')\">enable<\/a><\/li>\n              <li><a class='oha' onclick=\"_gkmh_linkCont('A_height')\">height<\/a><\/li>\n              <li><a class='oha' onclick=\"_gkmh_linkCont('A_id')\">id<\/a><\/li>\n              <li><a class='oha' onclick=\"_gkmh_linkCont('A_init')\">init<\/a><\/li>\n              <li class='fail'>maxHeight<\/li>\n              <li class=\"fail\">maxWidth<\/li>\n              <li class=\"fail\">minHeight<\/li>\n              <li class=\"fail\">minWidth<\/li>\n              <li class=\"fail\">preserveRatio<\/li>\n              <li class=\"fail\">resiza