dGridTable= gk.get(\"dndGridTable\");\n    \/\/插入GUL到codeMirror\n    insertGul2CodeMirror(dndGridTable);\n         eval(\"delete gk.designWizard\");  \n    gk.set('designWizardGrid.visible',false);\n    \n    });\n    }\n    \n  <\/js>\n  <js id='_gkmd_isDisableHeading'>\n     var isheading = gk.get(\"isheading\");\n      gk.event(\"com:#\"+isheading+\":heading.enable\");\n  <\/js>  \n  <window draggable='true' id='designWizardGrid' heading='Grid 設計精靈' layout='fit' width='800' height='480' modal='true'>\n    <tabPanel id='grid_tp' select='GridTab1'>\n      <tab id='GridTab1' name='編輯畫面' layout='fit' >\n        \n        <form id='designWizardGridForm' headerVisible='false' style='padding:0px'>\n          <toolbar id='gridSavetool'   separator='true'>\n            <field type='btn' label='完成' onClick='js:finishWizardGrid'\/>\n            <field type='btn' label='重置' onClick='js:clearGrid'\/>\n            <field type='btn' label='取消' onClick='js:closeGridWizard'\/>\n            <field type='btn' label='預覽' id='dndGrid_view' onClick='com:#GridTab2:grid_tp.select,bean:gkswDesignWizard.addGrid:designWizardGridList,designWizardGridForm'\/>  \n          <\/toolbar>\n         <formRow widthConfig='50:10%,50:15%,5:11%,50:10%,0:19%,80:15%,50:10%,40:10%'>\n            <field type='checkBox'  id='isPage' columnHeader='checkBox' label='是否分頁' boxlabel='' name='isPage' value='Y' checked='true'\/>\n            <field width='50' type='comboBox' label='每頁筆數' name='pageSize' value='10'>5:5,10:10,20:20,30:30<\/field>\n            <field type='btn' label='新增欄位' onClick='js:addOneRow'\/>  \n            <field type='checkBox'  id='isheading' columnHeader='checkBox' onchange='js:_gkmd_isDisableHeading' label='顯示表頭' boxlabel='' name='isheading' value='Y' checked='true' \/>\n            <field type='txt' id='heading' empty='請輸入表頭名稱'  label=''  name='heading' width='130'\/>\n            <field type='checkBox'  id='isGridShowCheckBox' columnHeader='checkBox' label='顯示checkBox' boxlabel='' name='isGridShowCheckBox' value='Y' checked='true'\/>\n            <field type='checkBox'  id='isAutoNewRow' columnHeader='checkBox' label='自動增列' boxlabel='' name='isAutoNewRow' value='Y' checked='true'\/>\n             <field type='checkBox'  id='isInitRow' columnHeader='checkBox' label='預設列' boxlabel='' name='isInitRow' value='Y' checked='true'\/>\n          <\/formRow>\n            <grid id='designWizardGridList' width='100%' headerVisible='true' height='350' type='edit'  checkBox='false'>\n              <field type='txt' name='dndGrid_colName'  label='欄位名稱'\/>\n              <field type='comboBox' name='dndGrid_colType' editable='false' label='欄位型態' value='label'>\n                Label:label,文字:txt,CheckBox:checkBox,下拉選單:comboBox,日期:date,按鈕:btn,文字區域:textarea,圖片:img,列表:list,數值:num,時間:time,年月:ym,radioBox:radioBox,滑動條:slider,數值選單:spin,觸發元件:trigger\n              <\/field>\n              <field type='txt' name='dndGrid_colValue'    columnWidth='50' allowBlank='true'  empty='AUTO' label='預設值'\/>\n              <field type='txt' name='dndGrid_colWidth'    columnWidth='30' allowBlank='true'  empty='AUTO' label='欄位寬度'\/>\n              <field type='btn' columnHeader='刪除' icon='icsc-delete' columnWidth='23' width='23' onClick='js:deleteOneRow'\/>\n            <\/grid>\n        <\/form>\n        \n      <\/tab>\n      <tab id='GridTab2' name='預覽畫面' layout='fit' onclick='com:#GridTab2:grid_tp.select,bean:gkswDesignWizard.addGrid:designWizardGridList,designWizardGridForm'>\n        <panel layout='fit' style='background-color:#ffffff' borders='true' onChange='show:demoPanel'>\n          <panel id='gridView' layout='fit' style='padding:12px;background-color:#777777' >\n            <page layout='fit'>\n              <panel id='$GridId' layout='fit' ><畫面預覽\/><\/panel>\n            <\/page>\n          <\/panel>\n        <\/panel>\n      <\/tab>\n      <field id='dndGridTable'  type='txt' label=''\/>\n      <field id='dndGridTable_mode'  type='txt' label='' value='2'\/>\n      \n    <\/tabPanel>\n  <\/window>\n  \n  \n<\/page>","loginPanel":"<page>\n  <window heading='登入視窗'  modal='true' id='_gk_login' width='300' height='150' layout='fit' closable='false' >\n    <form id='_gk_loginForm' headerVisible='false' width='200' frame='false'\n    style='padding:20px 25px 5px 25px'>\n      <field id='_gk_loginName' type='txt' name='userId' label='帳號*' value='' regex='[a-z||0-9]+' format='lower' title='請輸入小寫英數字' \/>\n      <field id='_gk_loginPwd' type='txt' pwd='true' name='pwd' label='密碼*' value='' \/>\n      <field type='adapt' space='10' label='&lt;a href=\"#\" onClick=\"javascript:forgetPwd();\"&gt;忘記密碼?&lt;\/a&gt;'>\n        <field id='_gk_loginBtn' type='btn' label='登入' onclick='js:login' icon='.\/html\/gwt\/res\/gk\/portal\/icon\/login.png'\/>\n        <field id='_gk_passwdBtn' type='btn' label='修改密碼' icon='.\/html\/gwt\/res\/gk\/portal\/icon\/key2.png' \n        onclick='com:#false:_gk_login.visible,com:#true:_gk_passwd.visible,js:openPasswd'\/>\n      <\/field>\n    <\/form>\n  <\/window>\n  <js init='js:this'>\n    function showLoginPanel(){\n      gk.set('_gk_login.visible',true);\n      \/\/ $('#_gk_loginBtn').css('padding','10px 0px 0px 177px');\n      \/\/setTimeout(\"$('#_gk_loginName-input').focus();\",500);\n      setTimeout(function (){document.getElementById('_gk_loginName-input').focus();},500);\n      $('#_gk_loginName-input').keypress(\n\tfunction(event){ if(event.keyCode=='13'){ gk.event('js:login'); }}\n      );\n      $('#_gk_loginPwd-input').keypress(\n\tfunction(event){ if(event.keyCode=='13'){ gk.event('js:login'); }}\n      );\n    }\n    function hideLoginPanel(){\n      gk.set('_gk_login.visible',false);\n    }\n    function hidePasswdPanel(){\n      gk.set('_gk_passwd.visible',false);\n    }   \n    \n    var hide = gk.getParameter('hideLogin');\n    if(hide != 'true') {\n      gk.event('bean:gksaLogin.session');\n    }\n    function forgetPwd(){\n      gk.set('_gk_forgetPwd.visible','true');\n    }\n    \n    \/\/ 嘗試從Cookie取得login資訊,暫時方便登入的做法\n    var userId = gk.cookie('userId');\n    if (userId == undefined || userId == null || userId == 'null') {\n      \/\/gk.set('_gk_loginName', '');\n      \/\/gk.set('_gk_loginPwd', '');\n    } else {\n      gk.set('_gk_loginName', userId);\n      var cookPwd = gk.cookie('pwd');\n      $.getJSON(\"event\/put\/x\/crypto.decryptForClient.go?j={'i':{'pwd':'\"+cookPwd+\"'},'t':'map'}\", function(data) {\n        gk.set('_gk_loginPwd', data.i.cPwd);\n      });\n    }\n  <\/js>\n  <js id='login'>\n    <![CDATA[\n    gk.set('_gk_login.heading','登入視窗');\n    \/\/gk.set('_gk_loginForm.mask','登入中');\n\tif (gk.get('_gk_loginForm.validate')=='true') {\n\t\tgk.event('bean:gksaLogin.check:_gk_loginForm',function(data){\n          gk.cookie('userId',gk.get('_gk_loginName'));\n\t\t  gk.cookie('pwd',data.cLoginUserPwd);\n\t\t  if (data.isok) {\n\t\t    var url = data.desiredURL;\n\t\t    if (url != undefined && url != '') {\n\t\t      location = url;\n\t\t    } else {\n\t\t      location = 'http:\/\/uibuilder.icsc.com.tw\/portal\/index.gul';\n\t\t    }\n\t\t  }\n        });\n\t} else {\n\t    gk.set('_gk_login.heading','帳號或密碼錯誤，請重新輸入!');\n\t}\n    ]]>\n  <\/js>  \n  <js id='openPasswd'>\n    var rename = gk.get('_gk_loginName') == null ? '' : gk.get('_gk_loginName');\n    var repwd = gk.get('_gk_loginPwd') == null ? '' : gk.get('_gk_loginPwd');\n    gk.set('_gk_loginNameRe', rename);\n    gk.set('_gk_loginPwdRe', repwd);\n  <\/js>\n  \n  <window heading='修改密碼'  modal='true'\n  id='_gk_passwd' width='320' height='200' layout='fit' closable='false'>\n    <form id='_gk_passwdForm' headerVisible='false' frame='false' \n    style='padding:20px 10px 5px 10px'>\n      <formRow widthRate='130:100%'>\n        <field id='_gk_loginNameRe' type='txt' name='userId' label='帳號*' value='' regex='[a-z||0-9]+' format='lower' title='請輸入小寫英數字' \/>\n      <\/formRow>\n      <formRow widthRate='130:100%'>      \n        <field id='_gk_loginPwdRe' type='txt' pwd='true' name='pwd' label='密碼*' value='' \/>\n      <\/formRow>\n      <formRow widthRate='130:100%'>       \n        <field id='_gk_loginNewPwd' type='txt' pwd='true' name='newPwd' label='新密碼*' value='' \/>      \n      <\/formRow>\n      <formRow widthRate='130:100%'>       \n        <field id='_gk_loginNewPwdConfirm' type='txt' pwd='true' name='newPwdConfirm' label='再次確認新密碼*' value='' \/>      \n      <\/formRow>\n      <formRow widthRate='100:100%'>  \n        <field type='adapt' space='10'>\n\t\t  <field id='_gk_backToLoginBtn' type='btn' label='回登入畫面' icon='.\/html\/gwt\/res\/gk\/portal\/icon\/login3.png' onclick='com:#true:_gk_login.visible,com:#false:_gk_passwd.visible'\/>             \n\t\t  <field id='_gk_changePasswdBtn' type='btn' label='修改密碼' icon='.\/html\/gwt\/res\/gk\/portal\/icon\/key2.png' onclick='js:changePasswd' \/>     \n\t\t<\/field>\n      <\/formRow>  \n    <\/form>  \n    \n    <js id='changePasswd' >\n      \n      if(gk.get('_gk_loginNewPwd')!=gk.get('_gk_loginNewPwdConfirm')) {\n        gk.set('_gk_passwd.heading','新密碼輸入錯誤');      \n      } else {     \n        \/\/先修改密碼，再登入\n        gk.set('_gk_passwd.heading','修改密碼');\n        gk.set('_gk_passwdForm.mask','修改密碼中');\n        \n        gk.event('bean:gksaLogin.changePwd:_gk_passwdForm', function(data) {\n          gk.cookie('userId',gk.get('_gk_loginName'));\n          gk.cookie('pwd',data.cLoginUserPwd);\n\t  location = 'index.gul';\n        });\n      }   \n    <\/js>\n  <\/window>\n  \n  <window id='_gk_forgetPwd' layout='fit' width='350' height='220' modal='true'>\n    <form headerVisible='false' align='right'>\n      <toolbar id='forgettool' msgWidth='100%'\/>\n      <formRow widthRate='0:100%' height='40'>\n\t<field type='label' value='&lt;font size=\"3\" color=\"red\"&gt;※請輸入您註冊時所設定的問題答案，核對完成後會將密碼寄到您的信箱中!&lt;\/font&gt;'\/>\n      <\/formRow>\n      <formRow widthRate='100:100%'>\n\t<field type='label' label='Email:' \/>\n      <\/formRow>\n      <formRow widthRate='100:100%'>\n\t<field type='label' label='您自訂的問題:' value='我的生日?'\/>\n      <\/formRow>\n      <formRow widthRate='100:100%'>\n\t<field type='txt' label='您自訂的答案:'\/>\n      <\/formRow>\n      <field type='btn' label='確認' icon='false' style='padding:5px;' width='100' height='30' onclick='com:\"已將您的密碼寄送到信箱中\":forgettool'\/>\n    <\/form>\n  <\/window>\n<\/page>","xfield_textarea_override2":"<field type='txt'  regex='^([0-9a-zA-Z]([-.\\w]*[0-9a-zA-Z])*@([0-9a-zA-Z][-\\w]*[0-9a-zA-Z]\\.)+[a-zA-Z]{2,9})$' empty='請輸入email'\/>","navigation_tryit":"<page>\n  <js init='js:this'>\n    \/\/顯示當前模組(藍色)\n    $('#${navgation} div').find(':contains(\"${item}\")').css({'color':'#ed1941','cursor':'','font-weight':'bold'});\n    $('#${navgation} div div').find(':contains(\"${item}\")').parent().attr('onclick','');\n    $('#_gk_userId').click(function(){ \n      \/\/showLoginPanel();\n      location = 'login.gul';\n    });\n\n    \/\/使用者設定\n    function userProfile(){\n      \/\/gk.set(\"userprofile.visible\", \"true\");\n      gk.event(\"bean:userprofilebean.load\");\n    }\n    \/\/設定登入帳號,click進行登出\n    function _gk_setUserId(userId){\n      $('#_gk_userId').text(userId);\n      \/\/$('#_gk_userId').unbind('click');\n      $('#_gk_userId').click(function(){\n        gk.event('bean:gksaLogin.logout',function(){\n          showLoginPanel();\n          _gk_setUserId('未登入');\n        });\n      });\n    }\n    \/\/新功能,問題通報區域效果\n    $(document).ready(function(){\n      \/\/模組切換 mouserover, mouseout效果\n      $.each($('.ln'),function(index, value){\n        \/\/alert($(value).html());\n        $(value).css({'color':'#009ad6'});\/\/非當下模組與帳號的字體顏色\n        $(value).mouseover(function(){\n          if($(value).html().indexOf(\"${item}\")&lt;0){\n            $(value).css({'color':'#009ad6','background':'#fff','cursor':'pointer'});\n          }\n        });\n        $(value).mouseout(function(){\n          $(value).css({'color':'#009ad6','background':''});\n        });\n      });\n      $('#${navgation} div div').find(' :contains(\"${item}\")').parent().onclick=null;\n    });\n    \/\/取得專案下拉選單\n    \n    \/\/ 設定使用者\n    var hide = gk.getParameter('hideLogin');\n    if(hide != 'true') {\n      gk.event('bean:gksaLogin.session');\n    }\n    \n  <\/js>\n  \n  <panel  height='74' width='100%' layout='vrow'>\n    <loginPanel\/>\n    <window id='userprofile' height='215' width='350' heading='User Profile' modal=\"true\">\n      <form id='userProfileForm' align='right' headerVisible='false'>\n        <toolbar id='userProfTool' msgWidth='120'>\n          <field type='btn' id='userProfSave_btn' label='確認' onclick='bean:userprofilebean.save:userProfileForm' icon='.\/html\/gwt\/res\/gk\/portal\/icon\/ok.png'\/>\n          <field type='btn' id='chgPwd_btn' label='修改密碼' onclick='com:\"true\":chgpwd.visible' icon='.\/html\/gwt\/res\/gk\/portal\/icon\/key2.png'\/>\n        <\/toolbar>\n        <field type='txt' name='USERID' hidden='true' visible='false'\/>\n        <field type='txt' label='使用者姓名:' name='USERNAME'\/>\n        <field type='txt' label='連絡電話:' name='PHONE'\/>\n        <field type='txt' label='E-mail:' name='EMAIL' width='200'\/>\n        <field type='textarea' label='通訊地址:' name='ADDRESS' width='200'\/>\n      <\/form>\n    <\/window>\n    <row data='1,24,0'>\n      <hp style='background-color:#f0f0f0;font-size:13px;' id='${navgation}' >\n        <style>\n          .bulbOpen{z-index:2;border-top:solid 1px #ccc;border-right:solid 1px #ccc;border-left:solid 1px #ccc;background-color:white;\n          width:20px;height:24px;position:absolute;top:0px;right:5px;}\n          .bulbClose{z-index:2;width:20px;height:24px;position:absolute;top:0px;right:5px;}\n        <\/style>\n        <!-- 快速選單menu -->\n        <div style='position:absolute ;top:0px; left:0px; font-family:monospace; color:#00c; '>\n          <div class='ln' style='text-align:center;float:left; position:relative; top:2px; left:10px; width: 40px; height:24px' onclick=\"window.open('index.gul')\"><font style='vertical-align:middle'>首頁<\/font><\/div>\n          <div class='ln' style='text-align:center;float:left; position:relative; top:2px; left:10px; width: 60px; height:24px' onclick=\"window.open('designWizard.gul')\"><font style='vertical-align:middle'>線上教學<\/font><\/div>\n          <div class='ln' style='text-align:center;float:left; position:relative; top:2px; left:10px; width: 60px; height:24px' onclick=\"window.open('onLineHelp.gul#gulcheatsheet')\"><font style='vertical-align:middle'>GUL字典<\/font><\/div>\n          <div class='ln' style='text-align:center;float:left; position:relative; top:2px; left:10px; width: 60px; height:24px' onclick=\"dataExport()\"><font style='vertical-align:middle;'>我的資料<\/font><\/div>\n        <\/div>\n        <!-- 帳號,專案,工具箱 -->\n        <div style='position:absolute; top:0px; left:220px; width:255px;'>\n          <!-- 使用者帳號 -->\n          <div class='ln' style='position:relative; left:10px; top:0px; float:left; vertical-align:middle; text-align:center; height:24px; width:60px;'>\n            <div id='_gk_userId' style='margin:3px 0 0px 2px;'>登入<\/div>\n          <\/div>\n        <\/div>\n      <\/hp>\n    <\/row>\n    <row data='1,50,0'>\n      <hp style='background-color:#fff;font-size:13px;' >\n        <!-- 專案圖示 -->\n        <div style='position:absolute; top:24px; left:0px;' >\n          <a href='\/' ><img  src='\/tryit\/html\/gwt\/res\/gk\/images\/uib\/ezoui.png'\/><\/a>\n        <\/div>\n        <div style='position:absolute; top:50px; right:5px;display:none;'>\n          <img src='.\/html\/gwt\/res\/gk\/images\/uib\/infochamp.jpg'\/>\n        <\/div>\n        <!-- 訊息runMsg -->\n        <div id='runMsg' style='display:none; position:absolute; top:10px; left:500px; z-index:5; background-color:#ffc20e; font-size:16px; font-family:monospace'>\n          <table>\n            <tr>\n              <td>\n                <img style='position:relative;padding:0px' src='.\/html\/gwt\/res\/gxt\/images\/default\/grid\/loading.gif' \/>\n              <\/td>\n              <td>\n                <b>執行中...<\/b>            \n              <\/td>\n            <\/tr>\n          <\/table>\n        <\/div><div style='padding:32px 0 0 203px;color:#909090;font-size:12px' id='version'><\/div>\n      <\/hp>\n    <\/row>\n  <\/panel>\n<\/page>","formToolbar":"<page>\n  <toolbar id='${id}' msgWidth='200' width=\"${width==''?'300':width}\" msg=\"${msg==''? 'true':'false'}\">\n    <field id='${id}_create_${binding}' type='btn' label='新增' icon='icsc-create' onClick='js:${id}_create'\/>\n    <field id='${id}_update_${binding}' type='btn' label='修改' icon='icsc-edit' onClick='js:${id}_update'\/>                  \n    <field id='${id}_delete_${binding}' type='btn' label='刪除' icon='icsc-delete' onClick='js:${id}_delete'\/>                 \n    <field id='${id}_read_${binding}' type='btn' label='查詢' icon='icsc-read' onClick='js:${id}_read'\/>                 \n  <\/toolbar>\n  <form id='${id}_form' headerVisible='false' visible='false'>\n    <field type='txt' name='grid' value='${grid}' visible='false'\/>\n  <\/form>\n  \n  <js id='${id}_create'>\n    gk.event('${id}.create.${binding}','com:#新增中...:${id},bean:${executeBean}:${binding},${id}_form',\n             function(data){\n               \n             });\n  <\/js>\n  \n  <js id='${id}_update'>\n    gk.event('${id}.update.${binding}','com:#修改中...:${id},bean:${executeBean}:${binding},${id}_form',\n             function(data){\n               \n             });\n  <\/js>\n  \n  <js id='${id}_delete'>\n    gk.event('${id}.delete.${binding}','com:#刪除中...:${id},bean:${executeBean}:${binding},${id}_form',\n             function(data){\n               \n             });\n  <\/js>\n  \n  <js id='${id}_read'>\n    gk.event('${id}.read.${binding}','com:#查詢中...:${id},bean:${executeBean}:${binding},${id}_form',\n             function(data){\n               \n             });\n  <\/js>  \n<\/page>","flot":"<page>\n  <hp id=\"${id}\" style='background-color:#FFFFFF' width='${width}' height='${height}'\/>\n  <js init='js:this'><![CDATA[\n    function loadJS(jsFile,callback){\n      if(jsFile.length==0) {\n        callback();    \n      }else{ \n        $.ajax({\n        url: jsFile[0],\n        dataType: \"script\",\n        success :function(){\n          jsFile.splice(0, 1);\n          loadJS(jsFile,callback);\n        }})\n       };\n     }\n\n    var jsFile = [\n    '\/portal\/html\/gwt\/res\/jquery\/excanvas.min.js',\n    '\/portal\/html\/gwt\/res\/jquery\/jquery.flot.js',\n    '\/portal\/html\/gwt\/res\/jquery\/jquery.flot.selection.js'];\n    loadJS(jsFile,\n    function(){\n     gk.event('js:${js}');\n    });    \n  ]]><\/js>\n  <\/page>","xfield_textarea_override1":"<field type='textarea' inputStyle='border-color:black; border-style:dotted;cursor:crosshair;font-family: verdana;'\/>","xfield_txt_Test":"<page>\n    <field type='txt'  inputStyle='color:blue'\/>\n<\/page>","gkDesignWizard_Form":"<page>\n  \n  <!--\n  <row data='1,1,20'>\n    <panel>\n      <field type='btn' label='開啟Form' onClick='js:openFormWizard'\/>\n    <\/panel>\n  <\/row>-->\n  \n  \n  <js init='js:this'> \n    var model = 2;\/\/ 1:設計精靈, 2:GKEditor\n    gk.designWizardForm = function openWindow(show){\n      gk.set('designWizardForm.visible',show);\n    }\n  <\/js>\n  \n  <js id='initForm'>\n    gk.set(\"form_otherSet.reset\");\n    gk.set(\"form_colrowSet.reset\");\n    gk.set(\"form_widthSet.reset\");\n  <\/js>\n  \n  <js id='openFormWizard'>\n    model = 1;\n    gk.set('designWizardForm.visible','true');\n    gk.event(\"js:initForm\");\n     document.getElementById('gkmd_formHead-input').focus();\n  <\/js>\n  <js id='closeFormWizard'>\n    gk.event(\"js:initForm\");\n    gk.set('designWizardForm.visible','false');\n  <\/js>\n  \n  <!--功能鈕：完成-->\n  <js id='dndFormBtn_finish'>\n    if (model==1) {\n    gk.event(\"js:finishForm\");\n    } else if (model==2) {\n    gk.event('bean:gkswDesignWizard.addForm:form_colrowSet,form_widthSet,form_otherSet',function(){\n    var dndFormTable= gk.get(\"dndFo