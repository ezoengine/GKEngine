lect>\n          <button class=\"x-btn-text\" type=\"button\" style=\"position: relative; left:-22px; width: 25px; height:23px; vertical-align:middle;\">\n            <img src=\".\/html\/gwt\/res\/gk\/portal\/icon\/search.png\" style=\"width: 16px; height: 16px; position: relative; left: 0px; top: 1px;\" border=\"0\" role=\"presentation\" class=\" x-btn-image\"\/>\n          <\/button>\n        <\/div>\n        <!-- 訊息runMsg -->\n        <div id='runMsg' style='display:none; position:absolute; top:10px; left:500px; z-index:5; background-color:#ffc20e; font-size:16px; font-family:monospace'>\n          <table>\n            <tr>\n              <td>\n                <img style='position:relative;padding:0px' src=\"\/portal\/html\/gwt\/res\/gxt\/images\/default\/grid\/loading.gif\" \/>\n              <\/td>\n              <td>\n                <b>執行中...<\/b>            \n              <\/td>\n            <\/tr>\n          <\/table>\n        <\/div><div style='padding:32px 0 0 203px;color:#909090;font-size:12px' id='version'><\/div>\n      <\/hp>\n      \n    <\/row>\n  <\/panel>\n  \n<\/page>","gkDesignWizard_TabPanel":"<page>\n  \n  <js id='addTabPanelOneRow'>\n    var grid = gk.get(\"designWizardTabList\");\n    var len = grid.length;    \n    var colname=\"Tab\"+(len+1);\n    grid.push({\"dndGrid_colName\":colname});\n    gk.set(\"designWizardTabList\",grid);\n  <\/js>\n  <js id='deleteTabpanelOneRow'>\n    var gridList = gk.get(\"designWizardTabList\");\n    var row = gk.get(\"designWizardTabList.row\");\n    var id = row.idx;\n    var newList = [] ;\n    for (x in gridList){\n      if(x!=id) newList.push(gridList[x]);\n    }\n    gk.set(\"designWizardTabList\",newList);\n  <\/js>  \n  <js id='clearTabPanelGrid'>\n    gk.event(\"js:initTabPanel\");\n  <\/js>\n  <js init='js:this'> \n    gk.event(\"js:initTabPanel\");\n    gk.designWizard = function openWindow(show){\n      gk.set('openTabPanelWizard.visible',show);\n    }\n  <\/js> \n  \n  <js id='openTabPanelWizardJS'>\n    gk.set('openTabPanelWizard.visible',true);\n    gk.set('dndTabPanel_mode','1');\n    gk.event(\"js:initTabPanel\");\n    \/\/var form = gk.get(\"designWizardTabPanelForm\");\n    \/\/form.tabPosition = \"top\";\n    \/\/gk.set(\"designWizardTabPanelForm\",form);\n    \n    \/\/ gk.set(\"_gkmd_tabPosition_top.reset\");\n    \/\/ gk.set(\"_gkmd_tabPosition_bottom.reset\");\n    \/\/gk.set('tabPanel_tp.select',\"TabPanel_tab1\");\n    \/\/gk.fire(\"TabPanel_tab1\",\"onClick\");\n    document.getElementById('_gkmd_empty-input').focus();\n  <\/js>\n  \n  <js id='closeTabPanelWizard'>\n    gk.set('openTabPanelWizard.visible',false);\n    \/\/gk.event('com:#false:designWizardGrid.visible');\n  <\/js>\n  \n  \n  <js id='initTabPanel'>\n    gk.set(\"designWizardTabList\",[\n      {\"dndGrid_colName\":\"Tab1\"},\n      {\"dndGrid_colName\":\"Tab2\"},\n      {\"dndGrid_colName\":\"Tab3\"}\n    ]);\n  <\/js>\n  \n  <js id='finishWizardTabPanel'>\n    \/\/1:DesignWizard 2: GKEditor\n    var dndTabPanel_mode= gk.get(\"dndTabPanel_mode\");\n    \n    \/\/alert(dndTabPanel_mode);\n    if(dndTabPanel_mode==\"1\"){\n      gk.event('js:finishTabPanel');\n    }else{\n      gk.event('bean:gkswDesignWizard.addTabPanel:designWizardTabList,designWizardTabPanelForm',function(){\n\tvar dndTabPanel= gk.get(\"dndTabPanel\");\n\t\/\/插入GUL到codeMirror\n\tinsertGul2CodeMirror(dndTabPanel);\n\tgk.set('openTabPanelWizard.visible',false);\n\t\n      });\n    }\n    \n  <\/js>\n  <!--功能鈕：預覽-->\n  <js id='dndTabPanel_preview'>\n    gk.event('bean:gkswDesignWizard.addTabPanel:designWizardTabList,designWizardTabPanelForm',function(data){\n      \n      gk.set(\"tabPanel_tp.select\",\"TabPanel_preview\");\n      var tpView = data.TabPanelView;\n      var TabPanel = gk.createUniqueId(tpView);\n      gk.set('dndTabPanel',TabPanel);\n      gk.set('TabPanel_Demo',TabPanel);\n\n       });\n  <\/js>\n  \n  \n<!--  <field type='btn' onclick='js:openTabPanelWizardJS' \/>-->\n  <window draggable='true' id='openTabPanelWizard' heading='Grid 設計精靈' layout='fit' width='600' height='400' modal='true'>\n    <tabPanel id='tabPanel_tp' select='TabPanel_tab1'>\n      <tab id='TabPanel_tab1' name='編輯畫面' layout='fit' >\n        \n        <form id='designWizardTabPanelForm' headerVisible='false' style='padding:0px'>\n          <toolbar id='tabPanelSavetool'   separator='true'>\n            <field type='btn' label='完成' onClick='js:finishWizardTabPanel'\/>\n            <field type='btn' label='重置' onClick='js:clearTabPanelGrid'\/>\n            <field type='btn' label='取消' onClick='js:closeTabPanelWizard'\/>\n            <field type='btn' label='預覽' id='tabPanel_view' onClick='js:dndTabPanel_preview'\/>  \n          <\/toolbar>\n          <formRow widthConfig='20:20%,60:50%,0:0%'>\n            <field id='_gkmd_addnewRow' type='btn' label='新增欄位' onClick='js:addTabPanelOneRow'\/>\n\t    <field type='adapt' label='Tab位置'>\n\t      <field id='_gkmd_tabPosition_top' type='radiobox' label='上' name='tabPosition' value='top' checked='true'\/>\n\t      <field id='_gkmd_tabPosition_bottom' type='radiobox' label='下' name='tabPosition' value='bottom'\/>\n\t    <\/field>\n\t    <field type='txt' id='_gkmd_empty'\/>\n          <\/formRow>\n\t  <grid id='designWizardTabList' width='100%' headerVisible='true' height='350' type='edit'  checkBox='false'>\n\t    <field type='txt' name='dndGrid_colName'  label='欄位名稱'\/>\n\t    <field type='btn' columnHeader='刪除' icon='icsc-delete' columnWidth='23' width='23' onClick='js:deleteTabpanelOneRow'\/>\n\t  <\/grid>\n        <\/form>\n        \n      <\/tab>\n      <tab id='TabPanel_preview' name='預覽畫面' layout='fit' onclick='js:dndTabPanel_preview' >\n        <panel layout='fit' style='background-color:#ffffff' borders='true' >\n          <panel id='TabPanel_Demo' layout='fit' style='padding:12px;background-color:#777777'>\n            <page layout='fit'>\n              <panel layout='fit' ><畫面預覽\/><\/panel>\n            <\/page>\n          <\/panel>\n        <\/panel>\n      <\/tab>\n      <field id='dndTabPanel'  type='txt' label=''\/>\n      <field id='dndTabPanel_mode'  type='txt' label='' value='2'\/>\n      \n    <\/tabPanel>\n  <\/window>\n  \n  \n<\/page>","sheng_calendar":"<page>\n  <hp>\n    <iframe src=\"https:\/\/www.google.com\/calendar\/embed?height=600&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=${gmail}%40gmail.com&amp;color=%23060D5E&amp;ctz=Asia%2FTaipei\" \n     style=\" border-width:0 \" \n     width=\"${width}\" height=\"${height}\" \n     frameborder=\"0\" scrolling=\"no\">\n    <\/iframe>\n  <\/hp>\n<\/page>","css":"<page>\n  <js init='js:this'><![CDATA[\n    var url = gk.ctx +'\/html\/gwt\/res\/css\/icsc-gxt-${size}px.css';\n    if(document.createStyleSheet){\n      document.createStyleSheet(url);\n    }else {    \n      $('head').append('<link rel=\"stylesheet\" href=\"'+url+'\" type=\"text\/css\" \/>');\n    }\n    ]]><\/js>\n<\/page>","gkVoteStar":"<page>\n  <hp>\n    <div id='${id}' value=\"${value==''?'0':value}\">\n      <img width='24' src='starOff.png' num='1' class='start'\/>\n      <img width='24' src='starOff.png' num='2' class='start'\/>\n      <img width='24' src='starOff.png' num='3' class='start'\/>\n      <img width='24' src='starOff.png' num='4' class='start'\/>\n      <img width='24' src='starOff.png' num='5' class='start'\/>\n    <\/div>\n  <\/hp>\n  <js init='js:this'><![CDATA[\n    var starObj = $('#${id}');\n    var starValue = starObj.attr('value');\n    var star = starObj.children();\n    for(var i=0;i<5;i++){\n      $(star[i]).attr('src',i<starValue? 'starOn.png':'starOff.png');\n    }\n    starObj.click(function(){\n      var obj = event.srcElement ? event.srcElement : event.target;\n      var num = obj.getAttribute('num');\n      starObj.attr('value',num);\n      var star = $(this).children();\n      for(var i=0;i<5;i++){\n\t$(star[i]).attr('src',i<num? 'starOn.png':'starOff.png');\n      }\n      \/\/callout\n      var callOut = '${onclick}';\n      if(callOut.trim().length>0){\n        gk.event(callOut);\n      }\n    });\n    ]]><\/js>\n<\/page>","libComboCombo":"<page>\n  <field type='adapt' label='${label}'>\n    <field type='comboBox' id='box1' onSelect='js:selectB1'>a,b<\/field>\n    <field type='comboBox' id='box2'><\/field>\n  <\/field>\n  <js init='js:this'>\n  <\/js>\n  <js id='selectB1'>\n    var jdata= [{'text':'avalue','value':'a'},{'text':'bvalue','value':'b'}];\n    gk.set('box2.list',jdata);\n    var selected = gk.get('box1.select');\n    gk.set('box2.select',selected['value']);\n  <\/js>\n<\/page>"}